<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.auscope.portal.server.web.security.ANVGLAuthority" table="authorities">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="authority"/>
        <many-to-one name="parent" column="userId" not-null="true"></many-to-one>
    </class>
</hibernate-mapping>
